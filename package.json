{
  "name": "numericalz-internal-app",
  "version": "1.0.0",
  "description": "Internal Management System for Numericalz - UK Accounting Firm",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:performance": "cross-env NODE_ENV=development ANALYZE=true next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:profile": "next build --profile",
    "start": "next start",
    "start:performance": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=.*\\.integration\\.test\\.(ts|tsx)$",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:api": "jest --testMatch='**/tests/api/**/*.test.ts'",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "db:migrate-production": "node scripts/migrate-production.js",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-backup.js",
    "db:backup-schedule": "node scripts/schedule-backups.js",
    "db:migrate-safe": "node scripts/safe-migrate.js",
    "db:audit": "node scripts/database-audit.js",
    "db:monitor": "node scripts/monitor-database.js",
    "fix:vat-dates": "node scripts/fix-vat-return-dates.js",
    "force-logout-all": "node scripts/force-logout-all.js",
    "performance:test": "node scripts/performance-monitor.js",
    "performance:monitor": "node scripts/performance-monitor.js",
    "invalidate-jwt-tokens": "node scripts/invalidate-jwt-tokens.js",
    "analyze": "cross-env ANALYZE=true next build",
    "fix-vat-quarters": "node scripts/fix-vat-quarters.js",
    "analyze:server": "cross-env ANALYZE=true BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env ANALYZE=true BUNDLE_ANALYZE=browser next build",
    "performance:audit": "lighthouse http://localhost:3000 --chrome-flags='--headless' --output=html --output-path=./performance-audit.html",
    "performance:report": "node scripts/generate-performance-report.js",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:fonts": "node scripts/optimize-fonts.js",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:check": "npx npm-check-updates",
    "clean": "rm -rf .next out dist coverage",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "vat:auto-assign": "node scripts/auto-assign-vat-partners.js",
    "cron:start": "node scripts/setup-cron-jobs.js",
    "debug-ct": "node scripts/debug-ct-calculation.js",
    "test-companies": "node scripts/test-companies-calculations.js",
    "test-email": "node scripts/test-email-service.js",
    "test-brevo": "node scripts/test-brevo-direct.js",
    "ltd:rollover": "node scripts/ltd-workflow-rollover.js",
    "revert:rjg-dates": "node scripts/revert-rjg-coachworks-dates.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.50.0",
    "@types/node-cron": "^3.0.11",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@upstash/ratelimit": "^0.4.4",
    "@upstash/redis": "^1.25.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.0",
    "critters": "^0.0.23",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "node-cron": "^4.1.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "openai": "^5.6.0",
    "pg": "^8.16.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^3.5.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@jest/types": "^29.6.3",
    "@next/bundle-analyzer": "^14.0.4",
    "@playwright/test": "^1.40.1",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.7.1",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.2",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "accounting",
    "internal-management",
    "nextjs",
    "typescript",
    "prisma",
    "uk-accounting-firm",
    "performance-optimized"
  ],
  "author": "Numericalz Development Team",
  "license": "UNLICENSED"
}
